<% include ../_header %>

  <div class="main_wrapper">
    <!-- top nav -->
    <% include ../_topnav %>

      <div class="container-fluid">
        <div class="row">

          <% include ../_sidebar %>

            <!-- contents strt -->
            <div class="contents">
              <div class="col-sm-9 col-sm-offset-3 col-md-9 col-md-offset-3 col-lg-10 col-lg-offset-2 main">
                <h1 class="text-center">Company Name</h1>
                <h1 class="page-header">Add User</h1>
                <div class="add_user">
                  <form>
                    <div>
                      <div>
                        Name:
                        <br>
                        <input maxlength="100" type="text" required="required" class="box" placeholder="Type your name" />
                      </div>
                      <div>
                        Email:
                        <input maxlength="100" type="email" required="required" class="box" placeholder="Type your mail" />
                      </div>
                      <div>
                        Password:
                        <input maxlength="50" type="password" required="required" class="box" placeholder="Type your password" />
                      </div>
                      <div>
                        User type:
                        <select class="box">
                          <option>Admin</option>
                          <option>Manager</option>
                          <option>Employee</option>
                        </select>
                      </div>
                      <div>
                        <input type="reset" value="reset" class="button" />
                        <input type="submit" value="submit" class="button" />
                      </div>
                    </div>
                  </form>
                </div>
              </div>
            </div>
            <!-- contents end -->

        </div>
      </div>

  </div>

  <% include ../_footer %>
